@{
    ViewBag.Title = "编号设置";
}
<div data-options="region:'north',border:false" style="height:93px;">
    <div class="queryframe">
        <table>
            <tr>
                <td>编号代码：</td>
                <td>
                    <input type="text" name="serialno" id="serialno" class="textbox ui-box" />
                </td>
                <td>编号名称：</td>
                <td>
                    <input type="text" name="serialname" id="serialname" class="textbox ui-box" />
                </td>
                <td>
                    <a href="#" class="easyui-linkbutton" id="btnQuery" data-options="iconCls:'icon-search'" style="width:80px">Search</a>
                </td>
            </tr>
        </table>
    </div>
    <div class="toolbar">@Html.Raw(ViewBag.ToolBar)</div>
</div>
<div data-options="region:'center',border:false">
    <table id="list" class="scroll" cellpadding="0" cellspacing="0"></table>
</div>
<div id="editframe"></div>
@section scripts{
    <script type="text/javascript" src="@Url.Content("~/Content/SysSet/serialset.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            initQuery();
            initRefresh();
            initShowAdd();
            initShowUpdate();
            initDelete();
            initTest();
        });
    </script>
}
