<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/jquery-easyui-1.3.6/themes/default/easyui.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/jquery-easyui-1.3.6/themes/icon.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/site.css?v=3")" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
</head>
<body class="easyui-layout">
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    <script src="@Url.Content("~/Content/jquery-easyui-1.3.6/jquery.easyui.min.js")"></script>
    <script src="@Url.Content("~/Content/jquery-easyui-1.3.6/easyloader.js?locale=zh_CN&theme=default")"></script>
    <script src="@Url.Content("~/scripts/jquery.jdirk.js")"></script>
    <script src="@Url.Content("~/scripts/jeasyui.extensions.js")"></script>
    <script src="@Url.Content("~/Scripts/core.js")"></script>
    <script src="@Url.Content("~/Content/Common/common.js")"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
